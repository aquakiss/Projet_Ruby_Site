<%= simple_form_for(@point) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :x %>
    <%= f.input :y %>
    <%= f.input :map_id %>
  </div>

  <%= f.hidden_field(:photo_url_cache) %>
  <%= f.file_field(:photo_url) %>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  
<% end %>
