<%= simple_form_for(@point) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :x %>
    <%= f.input :y %>
    <%= f.input :map_id %>
    <%= f.input :photo_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <div class="form-group">
      <label for="exampleInputFile">Picture input</label>
      <input type="file" id="exampleInputFile">
  </div>
  
<% end %>
